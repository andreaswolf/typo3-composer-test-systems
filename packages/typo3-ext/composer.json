{
  "name": "andreaswolf/test-extension",
  "description": "Test extension for composerized TYPO3 test systems",
  "type": "typo3-cms-extension",
  "require": {
    "symfony/filesystem": "^5.4 || ^6.4 || ^7.0",
    "thecodingmachine/safe": "^2.5",
    "typo3/testing-framework": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "AndreasWolf\\TestExtension\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndreasWolf\\TestExtension\\Tests\\": "Tests"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "test_extension"
    }
  }
}
